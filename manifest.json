{
  "manifest_version": 3,
  "name": "Gemini 批量生图插件",
  "version": "2.1",
  "description": "自动执行 Gemini 作图提示词队列，附带侧边栏、进度条、计时器和日志",
  "action": {
    "default_title": "切换侧边栏"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://gemini.google.com/*"
      ],
      "css": [
        "sidebar.css"
      ],
      "js": [
        "prompts.js",
        "automation.js",
        "sidebar.js"
      ]
    }
  ]
}